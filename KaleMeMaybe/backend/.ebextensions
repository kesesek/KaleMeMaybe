files:
    "/etc/nginx/conf.d/01_rewrite.conf":
    mode: "000644"
    owner: root
    group: root
    content: |
      location / {
          try_files $uri $uri/ /index.html;
      }
    "/etc/nginx/conf.d/proxy.conf":
    mode: "000644"
    owner: "root"
    group: "root"
    content: |
      proxy_connect_timeout 600s;
      proxy_send_timeout 600s;
      proxy_read_timeout 600s;
      send_timeout 600s;